{
    "name": "quocdaijr/cmsqdjr",
    "type": "project",
    "description": "CmsQDJr is written in Laravel Framework.",
    "keywords": ["framework", "laravel", "quocdaijr", "cmsqdjr"],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "barryvdh/laravel-ide-helper": "^2.10",
        "elasticsearch/elasticsearch": "^7.14",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.6",
        "jeremykenedy/laravel-logger": "^5.0",
        "laravel/framework": "^8.40",
        "laravel/horizon": "^5.7",
        "laravel/telescope": "^4.6",
        "laravel/tinker": "^2.5",
        "laravelcollective/html": "^6.2",
        "league/flysystem-cached-adapter": "^1.1",
        "mcamara/laravel-localization": "^1.6",
        "nwidart/laravel-modules": "^8.2",
        "quocdaijr/administration": "*@dev",
        "quocdaijr/category": "*@dev",
        "quocdaijr/core": "*@dev",
        "quocdaijr/dashboard": "*@dev",
        "quocdaijr/file": "*@dev",
        "quocdaijr/post": "*@dev",
        "quocdaijr/tag": "*@dev",
        "vladimir-yuldashev/laravel-queue-rabbitmq": "^11.3"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Modules\\": "modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "pre-install-cmd": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "./modules/*"
        }
    ]
}
